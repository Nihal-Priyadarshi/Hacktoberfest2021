public class diagonalDifference {
    public static void main(String[] args){

        int n = 3;

        int arr[][] =
                {
                        {11, 2, 4},
                        {4 , 5, 6},
                        {10, 8, -12}
                };

        int d1 = 0,d2 = 0;
        for (int i = 0;i<n;i++){
            d1 = d1+arr[i][i];
            d2 = d2+arr[i][n-1-i];
        }

        int diff = Math.abs(d2-d1);

        System.out.println(diff);
    }
}
